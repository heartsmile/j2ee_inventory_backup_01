	<queryString>
		<![CDATA[SELECT MAX(s.Date) AS date, s.ProductID, p.ProductName, pu.UnitName, p.OrgPrice,
 st.StockName, (p.OrgPrice * SUM(s.Quantity)) as subTotal,  SUM(s.Quantity) as totalQuantity
FROM	stock_inventory s
LEFT JOIN product p ON s.ProductID = p.ProductID
LEFT JOIN stock st ON s.StockID = st.StockID
LEFT JOIN product_unit pu ON p.UnitID = pu.unitID

GROUP BY s.ProductID, s.StockID
ORDER BY s.StockID]]>
	</queryString>
